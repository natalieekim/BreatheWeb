{
    "manifest_version": 2,
    "name": "Password Generator",
    "version": "1.6",
    "description": "Generates strong passwords for users",
    "browser_action": {
        "default_icon": "img/toolbar.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "img/icon16.png",
        "32": "img/icon32.png",
        "48": "img/icon48.png",
        "64": "img/icon64.png",
        "96": "img/icon96.png",
        "128": "img/icon128.png"
    }
}